pipeline {
  agent any
   stages {
    stage ('Clean') {
      steps {
        sh '''#!/bin/bash
        echo "command to run the pkill.sh"
        '''
     }
   }
 stage ('Deploy') {
  steps {
     sh '''#!/bin/bash
     echo "command to run the setup2.sh"
     ssh ubuntu@10.0.102.138
     wget https://raw.githubusercontent.com/bjones519/Kura_C4_D5/master/setup2.sh
     chmod u+x setup.sh
     ./setup.sh
    '''
      }
    }
  }
}
